      animate={{ opacity: 1, y: 0 }}
      transition={shouldAnimate ? { duration: 0.5 } : { duration: 0 }}
      className="w-full bg-card rounded-3xl p-8 hover:shadow-2xl backdrop-blur-sm border border-border"
