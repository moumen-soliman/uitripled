   * Size of the notch
   * @default "md"
   */
  size?: "sm" | "md" | "lg";
  /**
   * Class name for the container
   */
  className?: string;
  /**
    * Variant of the notch.
    * "default": expands and pushes content.
    * "overlay": expands over content (absolute positioning).
    * @default "default"
    */
  variant?: "default" | "overlay";
}

export function NativeProfileNotch({
  imageSrc,
  name,
  username,
  children,
  size = "md",
